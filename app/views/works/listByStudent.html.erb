<h1><%= t('work_list_student')  %></h1>

<table>
  <tr>
    <th><%= t :course %></th>
    <th><%= t :node %></th>
    <th><%= t :initialpoints %></th>
  </tr>

<% for work in @works %>
  <tr>
    <td><%=h work.node.course.name %></td>
    <td><%=h work.initialpoints %></td>
    <td><%= link_to t('test'), work %></td>
  </tr>
<% end %>
</table>

<br />

